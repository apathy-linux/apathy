+++ b/archival/unzip.c
@@ -616,6 +616,7 @@ int unzip_main(int argc, char **argv)
 			break;
 
 		default:
+			xfunc_error_retval = 0;
 			bb_show_usage();
 		}
 	}
@@ -639,6 +640,7 @@ int unzip_main(int argc, char **argv)
 #endif
 
 	if (!src_fn) {
+		xfunc_error_retval = 0;
 		bb_show_usage();
 	}
 
