# 1
./configure \
 --build=$CBUILD         \
 --host=$CHOST           \
 --prefix=/usr           \
 --sysconfdir=/etc       \
 --mandir=/usr/share/man \
 --localstatedir=/var    \
 --without-included-zlib \
 --disable-ipv6          \
 --disable-locale        \
 --disable-lz4           \
 --disable-xxhash

make

# 2
doas -s
make install
rm -rfv \
 /usr/bin/rsync-ssl \
 /usr/share/man
