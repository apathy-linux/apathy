Subject: [PATCH 09/12] remove MAXSMP check for for mglru. (mss)

---
 mm/Kconfig | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/mm/Kconfig b/mm/Kconfig
index 8a3360591..5f3f22686 100644
--- a/mm/Kconfig
+++ b/mm/Kconfig
@@ -905,7 +905,7 @@ config LRU_GEN
 	bool "Multigenerational LRU"
 	depends on MMU
 	# the following options can use up the spare bits in page flags
-	depends on !MAXSMP && (64BIT || !SPARSEMEM || SPARSEMEM_VMEMMAP)
+	depends on 64BIT || !SPARSEMEM || SPARSEMEM_VMEMMAP
 	help
 	  A high performance LRU implementation for memory overcommit. See
 	  Documentation/vm/multigen_lru.rst for details.
